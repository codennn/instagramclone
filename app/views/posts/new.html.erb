<h1>New Post</h1>


<%= form_for @posts, html: { multipart: true} do |f| %>
    <%= f.label :image %>
    <%= f.file_field :image %> <br>
    <%= f.label :Title %>
    <%= f.text_area :Title %> <br>
    <%= f.label :description %>
    <%= f.text_area :description %> <br>
    
    <%= f.submit %>
<% end %>